    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}"
  },
  "devDependencies": {
    "@stencil/core": "^1.17.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/store": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6"
  },
  "license": "MIT",
  "author": "Ranjeet Singh",
  "dependencies": {
    "@types/jest": "^26.0.10",
    "firebase": "^7.19.1",
    "puppeteer": "^5.2.1"
  },
  "keywords": [
    "web components",
    "components",
    "stencil",
    "firebase",
    "firebase stencil",
    "stencil store"
