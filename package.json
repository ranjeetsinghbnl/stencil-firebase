{
  "name": "stencil-firebase",
  "private": false,
  "version": "0.0.2",
  "description": "Welcome to the Stencil + Firebase starter App. You can use this starter to build entire apps all with web components using Stencil + Firebase",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
